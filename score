#include<stdio.h>
#include<conio.h>
int main()
{
  int i,a[3],b[3],alicescore=0,bobscore=0;
  for(i=0;i<3;i++)
  {
    scanf("%d",&a[i]);
  }
  for(i=0;i,3;i++)
  {
    scanf("%d",&b[i]);
  }
for(i=0;i<3;i+++)
{
  if(a[i]>b[i])
      alicescore++;
  else if(a[i]<b[i])
       bobscore++;
 }
 printf("%d%d",alicescore,bobscore);
 return0;
}
